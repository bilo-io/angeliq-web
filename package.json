{
  "name": "vision-studio",
  "version": "0.0.2",
  "description": "A container for the vision platform apps",
  "main": "index.js",
  "engines": {
    "node": "10"
  },
  "homepage": ".",
  "build": {
    "appId": "io.vis-ion.studio",
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "index.html",
      "app.js",
      "package.json",
      "electron.js"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "target": "dmg",
      "icon": "dist/assets/images/favicon.ico"
    }
  },
  "scripts": {
    "start": "npm run dev",
    "commit": "node ./scripts/commit.js",
    "branch": "node ./scripts/branch.js",
    "release": "node ./scripts/release.js",
    "serve": "NODE_ENV=development nodemon ./server/index.js withCluster & yarn serve:browser",
    "serve:workers": "NODE_ENV=development nodemon withCluster ./server/index.js",
    "serve:browser": "node server/launch.js",
    "serve:storybook": "npx http-server ./docs/storybook",
    "start:electron": "npm run dev & electron .",
    "dev": "NODE_ENV=development webpack-dev-server -w --color --inline --open --config webpack.dev.js",
    "dev:electron": "concurrently \"BROWSER=none npm run dev\" \"wait-on http://localhost:8080 && electron .\"",
    "dev:dist": "webpack -p --colors --watch",
    "dev:coverage": "$(npm bin)/nyc --silent npm run start",
    "dev:storybook": "start-storybook -p 9000 -c .storybook",
    "build": "npm run build:dist",
    "build:dist": "NODE_ENV=production webpack -p --progress --config webpack.prod.js",
    "build:electron": "CSC_IDENTITY_AUTO_DISCOVERY=false npm run build --c.extraMetadata.main=electron.js",
    "build:storybook": "npm run clean:docs && build-storybook -c .storybook -o docs/storybook",
    "docker:build": "npx docker build -t vision-studio:dev .",
    "docker:start": "npx docker run -it -p 3000:3000 vision-studio:dev ",
    "nyc": "$(npm bin)/nyc report --reporter=text-summary",
    "test:e2e": "$(npm bin)/cypress open",
    "test:unit": "jest --coverage",
    "test:unit:watch": "jest --watch --coverage",
    "test:coverage": "NODE_ENV=test nyc --reporter=lcov --reporter=text mocha test/*.js",
    "test:ci": "$(npm bin)/start-server-and-test dev:coverage 8080 ci:cypress",
    "ci:cypress": "$(npm bin)/cypress run  --record --key fb31f4d3-6b9c-45a8-9c56-626484c4e3b5 --parallel",
    "ci:cypress:chrome": "npm run ci:cypress --browser chrome",
    "ci:test:unit": "jest --coverage",
    "ci:test:chrome": "$(npm bin)/start-server-and-test dev 8080 ci:cypress:chrome",
    "ci:cypress:coverage": "$(npm bin)/nyc report --reporter=text-summary",
    "test:cli": "$(npm bin)/start-server-and-test dev 8080 ci:cypress",
    "test:cli:chrome": "$(npm bin)/start-server-and-test dev 8080 ci:cypress:chrome",
    "clean": "npm run clean:dist",
    "clean:dist": "rm -rf dist",
    "clean:docs": "rm -rf demo",
    "posttest:ci": "npm run ci:cypress:coverage",
    "postci:cypress:coverage": "$(npm bin)/codecov -t $CODECOV_TOKEN -f '.nyc_output/processinfo/index.json'",
    "postbuild:electron": "bash ./scripts/electron-post-build.sh",
    "postbuild:dist": "rm -rf artifact && cp -r ./dist ./artifact"
  },
  "author": "Bilo Lwabona",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-2": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@cypress/code-coverage": "^3.5.3",
    "@cypress/webpack-preprocessor": "^5.1.2",
    "@fortawesome/free-brands-svg-icons": "^5.12.1",
    "@monaco-editor/react": "^3.4.0",
    "@storybook/addon-backgrounds": "^5.2.8",
    "@storybook/addon-cssresources": "^5.3.19",
    "@storybook/addon-knobs": "^5.2.8",
    "@storybook/addon-storysource": "^5.2.8",
    "@storybook/react": "^5.3.12",
    "@storybook/theming": "^5.2.8",
    "axios": "^0.19.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-runtime": "^6.26.0",
    "concurrently": "^5.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "cypress": "^4.11.0",
    "d3": "^5.14.2",
    "dom-to-image": "^2.6.0",
    "electron": "^9.1.1",
    "electron-builder": "^21.2.0",
    "eslint": "^7.5.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-formatter-gitlab": "^2.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-standard": "^4.0.1",
    "fast-average-color": "^6.2.0",
    "firebase": "^7.10.0",
    "highcharts": "^8.2.2",
    "highcharts-react-official": "^3.0.0",
    "highlight.js": "^9.18.1",
    "html-loader": "^0.5.5",
    "html2pdf.js": "^0.9.2",
    "istanbul-lib-coverage": "^3.0.0",
    "jest": "^25.4.0",
    "jspdf": "^1.5.3",
    "jsx-to-string": "^1.4.0",
    "leaflet": "^1.6.0",
    "loki": "^0.27.0",
    "markdown-loader": "^5.1.0",
    "marked": "^0.8.0",
    "moment": "^2.24.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "nyc": "^15.0.1",
    "openurl": "^1.1.1",
    "pdfkit": "^0.11.0",
    "pptxgenjs": "^3.1.1",
    "react": "^16.12.0",
    "react-big-calendar": "^0.21.0",
    "react-click-outside": "^3.0.1",
    "react-dom": "^16.12.0",
    "react-fontawesome": "^1.7.1",
